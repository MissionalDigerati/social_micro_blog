<div class="post-box resizable-video jp_unreached">
<div class="powered-by-box"> Powered by Joshua Project API</div>
	<div class="profile-box">
		<%= avatar(post, "http://www.joshuaproject.net") %>
        <div class="details">
			<%= link_to "Joshua Project", "http://www.joshuaproject.net", target: "_blank", class: 'profile-name' %><br>
		</div>
		<div class="clearfix"></div>
	</div>
	<div class="post_wrap">
    <%= post.content.html_safe %>
    <div class="clearfix"></div>
  </div>
  <div class="date-box"><%= post.creation_date %></div>
</div>